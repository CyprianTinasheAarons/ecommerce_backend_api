{
  "type": "sqlite",
  "database": "./db.sqlite3",
  "entities": [
    "./backend/src/models/users/*.ts",
    "./backend/src/models/products/*.ts",
    "./backend/src/models/orders/*.ts",
    "./backend/src/models/payments/*.ts",
    "./backend/src/models/cart/*.ts",
    "./backend/src/models/categories/*.ts",
    "./backend/src/models/shipping/*.ts"
  ],
  "synchronize": true
}
